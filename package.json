{
  "name": "callbag-distribute",
  "version": "2.0.0",
  "description": "callbag that distributes (shared) sources to many sinks",
  "main": "dist/index.cjs.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs.js",
      "default": "./dist/index.cjs.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/**/*.ts --format cjs,esm --dts --out-dir dist",
    "build:watch": "tsup --watch src/**/*.ts --format cjs,esm --dts --out-dir dist",
    "test": "npm run build && node dist/index.spec.js",
    "prepare": "npm run build",
    "prepublishOnly": "npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kwv/callbag-distribute.git"
  },
  "keywords": [
    "callbag"
  ],
  "author": "Kris Van de Steeg",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kwv/callbag-distribute/issues"
  },
  "homepage": "https://github.com/kwv/callbag-distribute#readme",
  "devDependencies": {
    "@types/node": "^18.0.0",
    "callbag-from-iter": "^1.3.0",
    "tsup": "^8.5.1",
    "typescript": "^5.1.6"
  }
}
